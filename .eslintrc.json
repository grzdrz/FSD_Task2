{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "quotes": [
            "error",
            "double"
        ],
        "max-len": 0,
        "linebreak-style": "off",
        "no-param-reassign": 0,
        "class-methods-use-this": 0,
        "import/no-unresolved": "off"
    }
}